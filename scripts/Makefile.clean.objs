include ${src_tree}/scripts/Makefile.include
-include ${KCONFIG_AUTOCONF}

PHONY		:=
obj-y		:= 

include Makefile

sub-dirs	:= $(patsubst %/,%,$(filter %/,${obj-y}))
PHONY		+= ${sub-dirs}

do_clean: ${sub-dirs}
	$Q${RM} *.o
	$Q${RM} *.a
	$Q${RM} *.elf
	$Q${RM} *.bin
	$Q${RM} .*.d

PHONY+=do_clean

${sub-dirs}:
	$Q${MAKE} -C $@ -f ${src_tree}/scripts/Makefile.clean.objs
	@${ECHO} $(call QUIET_CLEAN,$(abspath $@))

.PHONY: ${PHONY}
